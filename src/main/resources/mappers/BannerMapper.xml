<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.group.miShop.mapper.BannerMapper">

  <resultMap id="find" type="com.group.miShop.domain.entity.Banner">
    <id property="bannerId" column="banner_id" ></id>
    <result property="bannerImage" column="banner_image"></result>
  </resultMap>

  <select id="findAllBanner" resultMap="find">
    SELECT b.banner_image
    FROM banner b
  </select>
</mapper>